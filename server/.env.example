# Backend type: postgresql or supabase
BACKEND_TYPE=postgresql

# PostgreSQL configuration (local or Docker deployment)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=synq
POSTGRES_USER=synq_user
POSTGRES_PASSWORD=synq_password

# Supabase configuration (cloud deployment option)
# Set BACKEND_TYPE=supabase and configure these values for cloud deployment:
# SUPABASE_URL=https://project.supabase.co
# SUPABASE_KEY=anon_key
# SUPABASE_SERVICE_KEY=service_key
